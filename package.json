    "start-gameroom": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom.js",
    "start-gameroom2": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom2.js",
    "start-gameroom3": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom3.js",
    "start-gameroom4": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom4.js"
  },
  "author": "Jomshir98",
  "license": "MIT",
  "engines": {
    "node": ">= 12.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.170",
    "@types/node": "^14.14.35",
    "bondage-club-bot-api": "../bondage-club-bot-api/bondage-club-bot-api-v0.1.1.tgz",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "prom-client": "^13.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.28.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
