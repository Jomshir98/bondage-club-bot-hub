{
  "name": "bondage-club-bot-hub",
  "version": "1.0.0",
  "description": "Collection of bots created for Bondage Club",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "test": "tsc --noEmit -p . && eslint src",
    "start-maidspartynight": "yarn run build && node --trace-warnings --enable-source-maps build/index_maidspartynight.js",
    "start-gameroom": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom.js",
    "start-gameroom2": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom2.js",
    "start-gameroom3": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom3.js",
    "start-gameroom4": "yarn run build && node --trace-warnings --enable-source-maps build/index_gameroom4.js"
  },
  "author": "Jomshir98",
  "license": "MIT",
  "engines": {
    "node": ">= 12.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.6",
    "bondage-club-bot-api": "./bondage-club-bot-api-v0.2.0.tgz",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "prom-client": "^14.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  }
}
