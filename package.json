{
  "name": "bondage-club-bot-hub",
  "version": "1.0.0",
  "description": "Collection of bots created for Bondage Club",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "test": "tsc --noEmit -p . && eslint src",
    "start-maidspartynight": "yarn run build && node --trace-warnings --enable-source-maps build/index_maidspartynight.js",
    "start-iwouldnotmind": "yarn run build && node --trace-warnings --enable-source-maps build/index_iwouldnotmind.js",
    "start-roleplaychallenge": "yarn run build && node --trace-warnings --enable-source-maps build/index_roleplaychallenge.js",
    "start-kidnappers": "yarn run build && node --trace-warnings --enable-source-maps build/index_kidnappers.js",
    "start-mistresstrouble": "yarn run build && node --trace-warnings --enable-source-maps build/index_mistresstrouble.js",
    "start-scheduler": "yarn run build && node --trace-warnings --enable-source-maps build/scheduler.js"
  },
  "author": "Jomshir98",
  "license": "MIT",
  "engines": {
    "node": ">= 12.4.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.8",
    "axios": "^0.27.2",
    "bondage-club-bot-api": "link:../bondage-club-bot-api",
    "express": "^4.18.1",
    "lodash": "^4.17.20",
    "prom-client": "^14.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^5.33.1",
    "@typescript-eslint/parser": "^5.33.1",
    "discord-api-types": "^0.37.3",
    "eslint": "^8.22.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
