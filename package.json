{
  "name": "bondage-club-bot-hub",
  "version": "1.0.0",
  "description": "Collection of bots created for Bondage Club",
  "private": true,
  "scripts": {
    "build": "tsc -p .",
    "test": "tsc --noEmit -p . && eslint src",
